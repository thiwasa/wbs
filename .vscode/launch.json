{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Xdebug",
            "type": "php",
            "request": "launch",
            "port": 9003,
            "pathMappings": {"/wbs/wbsplanner": "wbs/wbsplanner/index.php"}
        },
        {
            "name": "Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:80/wbs/wbsplanner/index.php",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": false
        }       
    ]
}